{
  "presets": [
    ["@babel/env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      },
      "useBuiltIns": "usage"
    }],
    "@babel/stage-2"
  ],
  "plugins": [
    "@babel/transform-runtime",
    "syntax-dynamic-import"
  ],
  "comments": true
}
